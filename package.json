{
  "name": "alanode-portal-service-node",
  "version": "1.0.0",
  "description": "alanode Api",
  "main": "app/index.ts",
  "scripts": {
    "init": "npm install && npm run prisma:generate && npm run make:migration",
    "start": "node ./dist/index.js --env=production",
    "production": "pm2 start ./dist/index.js --env=production",
    "artisan": "node ./dist/console/artisan.js",
    "pm": "pm2 start ./app -i max",
    "stop": "pm2 stop all",
    "logs": "pm2 logs",
    "start:dev": "ts-node -r tsconfig-paths/register ./app/index.ts",
    "lint": "eslint . --ext .ts",
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node scripts/copyAssets",
    "tsc": "tsc",
    "dev:start": "npm-run-all build start",
    "dev-ts": "nodemon --exec @swc-node/register app",
    "start:inspect": "nodemon --watch app -e ts,ejs --exec \"node --require ts-node/register --inspect ./app\"",
    "test": "jest",
    "test:watch": "npm run test -- --watch --silent=false --detectOpenHandles",
    "swc": "swc",
    "make:migration": "prisma migrate dev --create-only",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "prisma:init": "prisma init",
    "convert:tsconfig": "ts-node scripts/convert-tsconfig-to-swc",
    "dev": "concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "swc --config-file=.swcrc app -w --out-dir dist",
    "watch-dev": "nodemon --delay 500ms --watch \"dist/**/*\" -e js ./dist",
    "build": "swc --config-file=.swcrc app -d dist"
  },
  "keywords": [
    "alanode"
  ],
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "events": {
    "restart": "kill-port 3000",
    "crash": "kill-port 3000"
  },
  "delay": "1500",
  "author": "Richard Torenvliet",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "@sentry/node": "^7.42.0",
    "@sentry/tracing": "^7.42.0",
    "@types/express-fileupload": "^1.4.1",
    "aws-sdk": "^2.1257.0",
    "axios": "^1.3.4",
    "base32": "^0.0.7",
    "bcrypt": "^5.1.0",
    "boxen": "4.0",
    "chalk": "2.4",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "csv-writer": "^1.6.0",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-jwt": "^7.7.7",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "nodemailer": "^6.8.0",
    "pg": "^8.8.0",
    "pg-boss": "^9.0.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "sharp": "0.31.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "unique-names-generator": "^4.7.1",
    "uuid": "^8.3.2",
    "winston": "^3.8.2",
    "winston-transport-sentry-node": "^2.7.0",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@swc-node/register": "^1.5.4",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.22",
    "@swc/jest": "^0.2.23",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.4",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.7",
    "@types/yargs": "^17.0.20",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "concurrently": "^7.6.0",
    "esbuild-plugin-tsc": "^0.3.1",
    "eslint": "^8.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "kill-port": "^2.0.1",
    "nodemon": "^2.0.20",
    "prisma": "^4.9.0",
    "supertest": "^6.3.2",
    "ts-jest": "^29.0.3",
    "tsconfig-to-swcconfig": "^2.0.1",
    "typescript": "^4.8.4"
  }
}
